#include <iostream> // Including the standard input-output stream
using namespace std; // Using the standard namespace

int main() {
    int num1, num2, sum; // Declaring three integer variables
    cout << "Enter two numbers: "; // Asking user for input
    cin >> num1 >> num2; // Taking input for two numbers
    sum = num1 + num2; // Adding the two numbers
    cout << "Sum: " << sum << endl; // Displaying the sum
    return 0; // Returning 0 to indicate successful execution
}
